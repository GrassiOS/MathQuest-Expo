{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@assets/*": ["assets/*"],
      "@features/*": ["Features/*"],
      "@core/*": ["Core/*"],
      "@ui/*": ["UI/*"],
      "@data/*": ["Data/*"],
      "@resources/*": ["Resources/*"],
      "@components/*": ["components/*"],
      "@contexts/*": ["contexts/*"],
      "@types/*": ["types/*"],
      "@utils/*": ["utils/*"],
      "@constants/*": ["constants/*"],
      "@data/*": ["data/*"]
    },
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
